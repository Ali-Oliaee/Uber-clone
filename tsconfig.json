{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    /* Visit https://aka.ms/tsconfig.json to read more about this file */

    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "./src/components/*"
      ],
      "@assets/*": [
        "./src/assets/*"
      ],
      "@navigation": [
        "./src/navigation/*"
      ],
      "@screens/*": [
        "./src/screens/*"
      ],
    },
    "skipLibCheck": true
  }
}
